{
  "name": "gabber-client-react-native",
  "version": "1.0.0",
  "main": "./lib/commonjs/index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "bob build",
    "generate": "rm -rf src/generated/* && openapi-generator-cli generate -i \"https://api.gabber.dev/openapi.yaml\" -g typescript-axios -o src/generated --additional-properties=withSeparateModelsAndApi=true,apiPackage=api,modelPackage=model",
    "lint": "eslint -f unix \"src/**/*.{ts,tsx}\""
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "devDependencies": {
    "eslint-config-react-app": "^7.0.1",
    "react-native-builder-bob": "^0.37.0",
    "eslint": "^8.57.1",
    "eslint-plugin-react": "^7.37.2"
  },
  "source": "./src/index.ts",
  "module": "./lib/module/index.js",
  "types": "./lib/typescript/commonjs/src/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./lib/typescript/module/src/index.d.ts",
        "default": "./lib/module/index.js"
      },
      "require": {
        "types": "./lib/typescript/commonjs/src/index.d.ts",
        "default": "./lib/commonjs/index.js"
      }
    }
  },
  "files": [
    "src",
    "lib",
    "!**/__tests__",
    "!**/__fixtures__",
    "!**/__mocks__"
  ],
  "react-native-builder-bob": {
    "source": "src",
    "output": "lib",
    "targets": [
      [
        "commonjs",
        {
          "esm": true
        }
      ],
      [
        "module",
        {
          "esm": true
        }
      ],
      [
        "typescript",
        {
          "esm": true
        }
      ]
    ]
  },
  "eslintIgnore": [
    "node_modules/",
    "lib/"
  ],
  "peerDependencies": {
    "react-native": ">=0.71.0",
    "@livekit/react-native": "^2.6.0",
    "react": "^18.0.0 || ^19.0.0",
    "@types/react": "^18.0.0 || ^19.0.0",
    "@types/react-native": "^0.71.0 || ^0.72.0",
    "typescript": "^5.7.3",
    "axios": ">=1"
  }
}